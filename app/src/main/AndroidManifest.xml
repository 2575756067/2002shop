<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.shopkuang">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name=".app.MyApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.brand.BrandShopActivity" />
        <activity
            android:name=".SplaceActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".ui.ChaneActivity.ChaneActivity" />
        <activity android:name=".ui.pinpai.BrandActivity" />
        <activity android:name=".ui.brand.BrandListActivity" />
        <activity android:name=".ui.home.NewsFirstActivity" />
        <activity android:name=".ui.Details.ShopDetailsActivity" />
        <activity android:name=".ui.topic.TopicDetailsActivity"/>
        <activity android:name=".ui.sort.SortActivity"/>
    </application>

</manifest>